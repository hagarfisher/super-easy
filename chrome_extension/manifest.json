{
  "name": "SuperEasy",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [ {
    "matches": ["https://*.primadonaonline.co.il/*"],
    "js": ["content-script.js"]
  } ]
}